    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "npm run clean && SHARP_IGNORE_GLOBAL_LIBVIPS=1 gatsby build",
    "develop": "npm run clean && SHARP_IGNORE_GLOBAL_LIBVIPS=1 gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
